n=6
n = n // 2
count = n
for i in range (n):
    for j in range (n, i, -1):
        if j != n:
            print ("*" + str (count),end="")
        else:
            print (count, end="")
    count -= 1
    print ()
count += 1
for i in range(n):
    for j in range(i+1):
        if j!=0:
            print("*"+str(count),end="")
        else:
            print(count,end="")
    count+=1
    print()
